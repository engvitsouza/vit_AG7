%Essa função multiplica o mesmo desvio randomico por todas dimensoes
p2 = p; % inicializo a pop
[N, K, numInd]=size(p);
for i = 1:numInd
    for j = 1:N   %para todos vetores
        for k = 1:K   %para todos vetores
            if(rand < Tmutacao)
                p2(j,k,i) = p(j,k,i) .* (desvio * randn);
            end
        end
    end
end
end